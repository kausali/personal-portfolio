                    <textarea name="message" id="messgae" cols="30" rows="10" placeholder="Message.."></textarea><br>
                    <button id="submit" type="submit">Send</button>
                </form> 
                    {{# if success}}
                        <div class="success">
                            <h3 class="success-font">Message Sent</h3>
                        </div>
                    
                    {{ else}}
                        {{# if errors}}
                            <div class="errors">
                                <ul> <span class="li-font"> Your message was not sent becasue:</span><br>
                                    {{# each errors}}
                                    <li class="li-font">{{ this.msg }}</li><br>
                                    {{/each}}
                                </ul>
                            </div>
                        {{/if}}
                    {{/if}}

            </div>
